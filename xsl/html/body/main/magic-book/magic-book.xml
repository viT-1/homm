<?xml version="1.0" encoding="UTF-8"?>
<script type="text/x-template" id="magic-book" data-xml="xsl/html/body/main/magic-book/magic-book">
	<div iam-magic-book="">
		<ul iam-spells="">
			<magic-spell v-for="spell in spells" v-bind="{spell: spell}" />
		</ul>
		<vue-ids-pager v-bind="{ ids: spellsIds }">
			<template v-slot="idsPager">
				<p>currentIds: {{idsPager.currentIds}}</p>
			</template>
		</vue-ids-pager>
		<slot name="nav"></slot>	
	</div>
</script>